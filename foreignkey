create table gaji(no_id int(4) not null,gaji bigint(10) not null,dari date not null, sampai date,foreign key(no_id) references pegawai(no_id));

create table Albums(AlbumId int primary key not null,AlbumName varchar(255),DataReleased datetime,ArtistId int not null,GenreId int not null, foreign key(ArtistId) references Artists(ArtistId),foreign key(GenreId) references Genre(GenreId));





