select * from pegawai \G //melihat data tidak dalam bentuk tabel
select * from pegawai where jenis_kel="P" and year(tgl_lahir)>1991;
select * from pegawai where email like "%yahoo.com"; 
select * form pegawai where name like "D___" //cari nama yg berawalan huruf d dilanjutkan 3 huruf
select * from pegawai where name like "D%" //cari nama yg berawalan huruf d

